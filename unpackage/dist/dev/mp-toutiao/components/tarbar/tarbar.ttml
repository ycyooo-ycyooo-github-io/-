<view class="tarbar"><view class="tarbar-list" style="{{'background:'+(tabBar.backgroundColor)+';'+('color:'+(tabBar.color)+';')+('border-top:'+(tabBar.position=='bottom'?'1rpx solid '+tabBar.borderStyle:0)+';')+('border-bottom:'+(tabBar.position=='top'?'1rpx solid '+tabBar.borderStyle:0)+';')}}"><view class="tarbar-list-ul"><block tt:for="{{tabBar.list}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['setSelected',['$0'],[[['tabBar.list','',index,'pagePath']]]]]]]}}" class="tarbar-list-li" bindtap="__e"><view class="tarbar-list-li-icon"><image src="{{selected==item.id?item.selectedIconPath:item.iconPath}}" mode></image></view><view class="{{(('tarbar-list-li-name')+' '+(selected==item.id?'active':''))}}">{{item.text}}</view></view></block></view></view></view>