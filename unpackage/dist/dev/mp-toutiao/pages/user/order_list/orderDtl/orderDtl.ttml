<view><view class="status">{{''+type+''}}</view><block tt:if="{{logisticShow}}"><view class="tui-flex-box"><image class="tui-icon-img" src="https://www.thorui.cn/wx/static/images/mall/order/img_order_logistics3x.png"></image><view class="tui-logistics"><view class="tui-logistics-text">{{traces.AcceptStation}}</view><view class="tui-logistics-time">{{traces.AcceptTime}}</view></view><view><tui-icon vue-id="1" name="arrowright" color="#b2b2b2" size="20" data-event-opts="{{[['^tap',[['toTrack']]]]}}" bind:tap="__e" bind:__l="__l"></tui-icon></view></view></block><view class="addr"><view class="icon"><image src="../../../../static/img/addricon.png" mode></image></view><view class="right"><view class="tel-name"><view class="name">{{''+recinfo.name+''}}</view><view class="tel">{{''+recinfo.phone+''}}</view></view><view class="addres">{{''+(recinfo.province+recinfo.city+recinfo.town)+"\n\t\t\t\t"+recinfo.address+''}}</view></view></view><view class="buy-list"><block tt:for="{{buylist}}" tt:for-item="row" tt:for-index="index" tt:key="index"><view class="row"><view class="goods-info"><view data-event-opts="{{[['tap',[['togoods',['$0'],[[['buylist','',index]]]]]]]}}" class="img" bindtap="__e"><image src="{{row.pro_sn}}"></image></view><view class="info"><view data-event-opts="{{[['tap',[['togoods',['$0'],[[['buylist','',index]]]]]]]}}" class="title" bindtap="__e">{{row.pro_name}}</view><view data-event-opts="{{[['tap',[['togoods',['$0'],[[['buylist','',index]]]]]]]}}" class="spec" bindtap="__e">{{"单价"+row.pro_price+" 数量:"+row.pro_num}}</view><view class="price-number"><view data-event-opts="{{[['tap',[['togoods',['$0'],[[['buylist','',index]]]]]]]}}" class="price" bindtap="__e">{{"￥"+row.subtotal}}</view><block tt:if="{{type=='已发货'||type=='已收货'}}"><view data-event-opts="{{[['tap',[['toround',['$0'],[[['buylist','',index]]]]]]]}}" class="default" bindtap="__e">退换</view></block></view></view></view></view></block></view><view class="order"><view class="row"><view class="left">积分 :</view><view class="right">{{'已扣除'+int+'积分抵扣0元'}}</view></view><view class="row"><view class="left">备注 :</view><view class="right"><input placeholder="选填,备注内容" data-event-opts="{{[['input',[['__set_model',['','note','$event',[]]]]]]}}" value="{{note}}" bindinput="__e"/></view></view><view class="row"><view class="left">订单编号:</view><view class="right"><text>{{id}}</text></view></view><view class="row"><view class="left">下单时间:</view><view class="right"><text>{{time}}</text></view></view><block tt:if="{{order_ship}}"><view class="row"><view class="left">快递方式:</view><view class="right"><text>{{shipname}}</text></view></view></block><block tt:if="{{orderShipSn}}"><view class="row"><view class="left">快递编号:</view><view class="right"><text>{{orderShipSn}}</text></view></view></block></view><view class="detail"><view class="row"><view class="nominal">商品总金额</view><view class="content">{{'￥'+$root.f0+''}}</view></view><view class="row"><view class="nominal">运费</view><view class="content">{{'￥+'+$root.f1+''}}</view></view><view class="row"><view class="nominal">优惠金额</view><view class="content">{{'￥-'+$root.f2+''}}</view></view><view class="row"><view class="nominal">合计</view><view class="content">{{'￥'+$root.f3+''}}</view></view></view><view class="blck"></view><view class="footer"><view class="settlement"><view class="sum">实付金额:<view class="money">{{"￥"+$root.f4}}</view></view></view></view></view>