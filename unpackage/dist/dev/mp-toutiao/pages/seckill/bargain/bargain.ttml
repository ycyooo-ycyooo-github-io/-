<view><block tt:if="{{tabIndex==0}}"><view class="group" style="padding-bottom:50rpx;"><view class="header"><image src="../../../static/img/banner.jpg" mode></image></view><view class="goods"><block tt:for="{{goodsList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['toCut',['$0'],[[['goodsList','',index]]]]]]]}}" class="goods-item" bindtap="__e"><view class="left"><image src="{{item.pro_img}}" mode></image></view><view class="right"><view class="info" style="height:50%;">{{item.pro_name}}</view><view class="tip"><view class="content"><text>包邮</text></view><block tt:if="{{item.bargain_num<=3}}"><view style="display:flex;justify-content:left;align-items:center;padding-right:10rpx;"><image src="../../../static/img/seckill/kj-on.png" mode></image><text style="font-size:26rpx;color:#E23438;">超容易砍</text></view></block></view><view class="bottom"><view class="price">{{'￥'+item.pro_price+''}}</view><view class="bottomRight"><view class="btn">{{'砍至'+item.dis_price+'元拿'}}</view></view></view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></block><block tt:else><view class="goods"><block tt:for="{{bartainlist}}" tt:for-item="item" tt:for-index="index" tt:key="index"><block tt:if="{{bartainlist.length>0}}"><view class="goods-item"><view data-event-opts="{{[['tap',[['toCut',['$0'],[[['bartainlist','',index]]]]]]]}}" class="left" bindtap="__e"><image src="{{item.pro_img}}" mode></image></view><view class="right"><view data-event-opts="{{[['tap',[['toCut',['$0'],[[['bartainlist','',index]]]]]]]}}" class="info" bindtap="__e">{{item.pro_name}}</view><block tt:if="{{item.etime>0&&item.pro_price-item.price_totle!=item.dis_price}}"><view class="progress"><progress percent="{{item.progress}}" activeColor="red" active stroke-width="15" border-radius="30" duration="10"></progress></view></block><view class="bottom"><block tt:if="{{item.etime<=0}}"><view><block tt:if="{{item.pro_price-item.price_totle==item.dis_price}}"><view class="success"><tui-icon vue-id="{{'1-'+index}}" name="circle-selected" color="#00E83A" size="20" bind:__l="__l"></tui-icon>成功</view></block><block tt:else><view class="error"><tui-icon vue-id="{{'2-'+index}}" name="about" color="#b2b2b2" size="20" bind:__l="__l"></tui-icon>砍价失败</view></block></view></block><block tt:else><view class="etime"><block tt:if="{{item.pro_price-item.price_totle==item.dis_price}}"><view class="success"><tui-icon vue-id="{{'3-'+index}}" name="circle-selected" color="#00E83A" size="20" bind:__l="__l"></tui-icon>成功</view></block><block tt:else><view style="display:flex;justify-content:left;align-items:center;"><tui-icon vue-id="{{'4-'+index}}" name="clock" color="#b2b2b2" size="16" bind:__l="__l"></tui-icon><tui-countdown vue-id="{{'5-'+index}}" time="{{item.etime}}" bcolor="#fff" color="#b2b2b2" bind:__l="__l"></tui-countdown><text style="font-size:22rpx;">后结束</text></view></block></view></block><view class="bottomRight"><block tt:if="{{item.pro_price-item.price_totle==item.dis_price}}"><view data-event-opts="{{[['tap',[['toUse',['$0'],[[['bartainlist','',index]]]]]]]}}" class="btn" bindtap="__e">去使用</view></block><block tt:else><view data-event-opts="{{[['tap',[['toCut',['$0'],[[['bartainlist','',index]]]]]]]}}" class="btn" bindtap="__e">去看看</view></block></view></view></view></view></block></block><view class="loading-text">暂无参与砍价的商品</view></view></block><view class="tabBar"><view class="tabBar-list"><block tt:for="{{tabBar}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['tabChange',['$0'],[[['tabBar','',index,'id']]]]]]]}}" class="tabBar-item" bindtap="__e"><view class="icon"><image src="{{tabIndex==item.id?item.iconPath:item.icon}}" mode></image></view><view class="{{(('name')+' '+(tabIndex==item.id?'active':''))}}">{{''+item.text+''}}</view></view></block></view></view></view>