page {
  background-color: #DB2A29;
  background-image: url("http://web.xmcpcn.com/img/banner/bg.png");
  background-repeat: no-repeat;
}
.cutpop .tui-modal-box {
  padding: 0 !important;
  background: none !important;
}
.cutpop .pop image {
  width: 100%;
  height: 670rpx;
}
.pageInfo {
  padding: 3%;
}
.pageInfo .rule {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: right;
    -webkit-justify-content: right;
            justify-content: right;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    padding-left: 30rpx;
    color: #fff;
    font-size: 28rpx;
    width: 145rpx;
    height: 50rpx;
    border: 1px solid #FFFFFF;
    border-radius: 50rpx;
    margin-bottom: 40rpx;
    margin-left: 75%;
}
.pageInfo .product {
    margin-top: 100rpx;
    background-color: #FFFFFF;
    padding: 30rpx;
    border-radius: 10rpx;
    text-align: center;
}
.pageInfo .product .logo {
      width: 150rpx;
      height: 150rpx;
      border-radius: 50%;
}
.pageInfo .product .path {
      color: #C72C50;
      padding-bottom: 40rpx;
      font-size: 30rpx;
      padding-top: 20rpx;
}
.pageInfo .product .good {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: left;
      -webkit-justify-content: left;
              justify-content: left;
      -webkit-box-align: start;
      -webkit-align-items: flex-start;
              align-items: flex-start;
}
.pageInfo .product .good image {
        width: 220rpx;
        height: 200rpx;
}
.pageInfo .product .good .info {
        text-align: left;
        margin-left: 40rpx;
}
.pageInfo .product .good .info .name {
          font-size: 30rpx;
          font-family: '\5FAE\8F6F\96C5\9ED1';
}
.pageInfo .product .good .info .price {
          color: #FD5856;
          font-weight: bold;
          font-size: 36rpx;
          font-family: '\5FAE\8F6F\96C5\9ED1';
          margin-top: 50rpx;
}
.pageInfo .btns {
    padding: 40rpx 0;
    text-align: center;
}
.pageInfo .btns .money {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      font-size: 24rpx;
      color: #fff;
      padding-top: 10rpx;
      margin-bottom: 40rpx;
}
.pageInfo .btns .money .dis_price {
        text-align: right;
}
.pageInfo .btns .tips {
      color: #FFFFFF;
      font-size: 28rpx;
      padding-bottom: 20rpx;
      letter-spacing: 2rpx;
}
.pageInfo .btns .btn {
      background: -webkit-linear-gradient(left, #F7DE77, #EFC848);
      background: linear-gradient(to right, #F7DE77, #EFC848);
      color: #8A172F;
      font-weight: bold;
      font-size: 36rpx;
      letter-spacing: 2rpx;
      margin-bottom: 40rpx;
}
.pageInfo .btns .time {
      font-size: 26rpx;
      color: #fff;
      opacity: 0.8;
      letter-spacing: 2rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
              justify-content: center;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      margin-bottom: 20rpx;
}
.pageInfo .btns .time .tui-countdown-item {
        width: 32rpx !important;
        background: none !important;
        border: none !important;
}
.pageInfo .btns .time .tui-countdown-colon {
        color: #fff !important;
}
.pageInfo .peoples {
    background-color: #FFFFFF;
    padding: 30rpx;
    padding-top: 0;
    border-radius: 10rpx;
    text-align: center;
}
.pageInfo .peoples .img {
      width: 350rpx;
      height: 90rpx;
      margin-top: -12rpx;
}
.pageInfo .peoples .list {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: left;
      -webkit-justify-content: left;
              justify-content: left;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      padding-bottom: 30rpx;
}
.pageInfo .peoples .list .avatar {
        width: 90rpx;
        height: 90rpx;
        border-radius: 50%;
}
.pageInfo .peoples .list .info {
        margin-left: 30rpx;
        width: 62%;
        text-align: left;
}
.pageInfo .peoples .list .info .name {
          font-size: 28rpx;
}
.pageInfo .peoples .list .info .time {
          font-size: 24rpx;
          color: #B2B2B2;
          padding-top: 10rpx;
}
.pageInfo .peoples .list .cutPrice {
        color: red;
        font-size: 28rpx;
        letter-spacing: 2rpx;
        font-weight: bold;
}
.pop {
  text-align: center;
}
.pop .title {
    color: #E65B34;
    font-size: 38rpx;
    font-weight: bold;
    margin-bottom: 50rpx;
}
.pop .cut {
    width: 70%;
    background: -webkit-linear-gradient(top, #F7E496, #F3C24A);
    background: linear-gradient(to bottom, #F7E496, #F3C24A);
    color: #C13C0F;
    font-size: 36rpx;
    letter-spacing: 2rpx;
    border-radius: 60rpx;
    height: 80rpx;
    line-height: 80rpx;
}

