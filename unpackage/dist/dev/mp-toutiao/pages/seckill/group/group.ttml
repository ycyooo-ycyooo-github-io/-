<view><view class="group" style="margin-bottom:130rpx;"><view class="header"><view class="left"><view class="title">超级品牌日</view><view class="time"><text>距离结束</text><tui-countdown vue-id="1" time="{{seckillTime}}" bcolor="#fff" color="#c03d38" bind:__l="__l"></tui-countdown></view></view><view class="right"><view class="num"><view style="padding-right:30rpx;font-size:26rpx;">{{''+num+'人正在拼'}}</view><view class="user"><block tt:for="{{user}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="img"><image src="{{item.avatar?item.avatar:'../../../static/img/user.png'}}" mode></image></view></block></view></view></view></view><view class="goods"><block tt:for="{{goodsList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['goJoin',['$0'],[[['goodsList','',index]]]]]]]}}" class="goods-item" bindtap="__e"><view class="left"><image src="{{item.pro_img}}" mode></image></view><view class="right"><view class="info"><view class="name">{{''+item.pro_name+''}}</view><view class="dis">{{''+item.discount+'折'}}</view><view class="tip"><text>{{item.group_num+"人团"}}</text><text>包邮</text></view></view><view class="bottom"><view class="price">拼团价<text class="num">{{item.dis_price}}</text>元</view><view class="btn">去拼团</view></view></view></view></block></view></view><tarbar vue-id="2" selected="{{1}}" bind:__l="__l"></tarbar></view>