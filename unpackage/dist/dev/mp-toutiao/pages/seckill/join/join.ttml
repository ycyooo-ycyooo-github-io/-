<block tt:if="{{goods}}"><view><view class="goods"><view data-event-opts="{{[['tap',[['toGoods',['$0'],['item']]]]]}}" class="goods-item" bindtap="__e"><view class="left"><image src="{{goods.pro_src}}" mode></image></view><view class="right"><view class="info"><view class="name">{{''+goods.pro_name+''}}</view><view class="dis">{{''+info.discount+'折'}}</view><view class="tip"><text>{{info.group_num+"人团"}}</text><text>包邮</text></view></view><view class="bottom"><view class="price">拼团价<text class="num">{{info.dis_price}}</text>元</view></view></view></view></view><view class="card"><view class="user"><block tt:for="{{user}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="img"><image src="{{item.avatar?item.avatar:'../../../static/img/user.png'}}" mode></image></view></block></view><block tt:if="{{status}}"><view><view class="time">剩余<tui-countdown style="margin-left:10rpx;margin-top:6rpx;" vue-id="1" time="{{seckillTime}}" bcolor="#fff" bind:__l="__l"></tui-countdown>结束</view><view><block tt:if="{{showBtn}}"><view><block tt:if="{{!type}}"><button class="invite" open-type="share">邀请微信好友参团</button></block><block tt:else><button data-event-opts="{{[['tap',[['jojn',['$event']]]]]}}" class="invite" bindtap="__e">去参团</button></block></view></block><block tt:if="{{!userInfo}}"><button style="height:70rpx;line-height:70rpx;font-size:30rpx;border-radius:50rpx;" type="warn" open-type="getUserInfo" withCredentials="true" data-event-opts="{{[['getuserinfo',[['getuserinfo',['$event']]]]]}}" bindgetuserinfo="__e">微信登录</button></block></view></view></block><block tt:else><view><button class="invite">拼团成功</button></view></block></view></view></block>