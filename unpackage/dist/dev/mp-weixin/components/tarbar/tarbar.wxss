
.tarbar {
	width: 100%;
	z-index: 9999;
	position: fixed;
}
.tarbar-list {
	width: 100%;
	background: #4d586f;
	position: fixed;
	left: 0;
	bottom: 0;
}
.tarbar-list-ul {
	width: 100%;
	height: 100%;
	padding: 10rpx 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	box-sizing: border-box;
}
.tarbar-list-li {
	width: 80rpx;
	height: 80rpx;
}
.tarbar-list-li-icon {
	width: 50rpx;
	height: 50rpx;
	margin: 0 auto;
}
.tarbar-list-li-icon image {
	width: 50rpx;
	height: 50rpx;
}
.tarbar-list-li-name {
	width: 100%;
	text-align: center;
	line-height: 30rpx;
	font-size: 25rpx;
	height: 30rpx;
}
.tarbar-list-li-center {
	width: 100rpx;
}
.tarbar-list-li-center .tarbar-list-li-icon,
.tarbar-list-li-center .tarbar-list-li-icon image {
	width: 90rpx;
	height: 60rpx;
}
.active {
	color: #c03d38;
}

