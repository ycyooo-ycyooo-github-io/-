<view><view class="content"><view class="row"><view class="nominal">收件人</view><view class="input"><input placeholder="请输入收件人姓名" type="text" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view></view><view class="row"><view class="nominal">电话号码</view><view class="input"><input placeholder="请输入收件人电话号码" type="text" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view></view><view class="row"><view class="nominal">所在地区</view><view class="input"><picker mode="region" value="{{region}}" custom-item="{{customItem}}" data-event-opts="{{[['change',[['bindRegionChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+region[0]+region[1]+region[2]+''}}</view></picker></view></view><view class="row"><view class="nominal">详细地址</view><view class="input"><textarea auto-height="true" placeholder="输入详细地址" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" value="{{address}}" bindinput="__e"></textarea></view></view></view><view data-event-opts="{{[['tap',[['save',['$event']]]]]}}" class="save" bindtap="__e"><view class="btn">修改地址</view></view></view>