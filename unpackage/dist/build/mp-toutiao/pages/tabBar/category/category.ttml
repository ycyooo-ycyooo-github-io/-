<view><view class="status" style="{{'position:'+(headerPosition)+';'}}"></view><view class="header" style="{{'position:'+(headerPosition)+';'}}"><view class="addr"><image class="logo" src="../../../static/img/logo.png" mode></image></view><view class="input-box"><input placeholder="熙美诚品" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" bindtap="__e"/><view class="icon search"></view></view></view><view class="place"></view><view class="category-list"><scroll-view class="left" scroll-y="true"><block tt:for="{{categoryList}}" tt:for-item="category" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['showCategory',['$0'],[[['categoryList','',index,'id']]]]]]]}}" class="{{(('row')+' '+(category.id==showCategoryIndex?'on':''))}}" bindtap="__e"><view class="text"><view class="block"></view>{{''+category.cate_name+''}}</view></view></block></scroll-view><scroll-view class="right" scroll-y="true" scroll-top="{{scrollTop}}" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]]]}}" bindscrolltolower="__e"><view class="header-info"><view class="cate"><scroll-view class="tab-view" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><view data-event-opts="{{[['tap',[['swichall',['$event']]]]]}}" class="{{(('tab-bar-item')+' '+(currentTab==0?'active':''))}}" style="margin-left:0;" catchtap="__e"><text class="tab-bar-title">全部</text></view><block tt:for="{{tabbar}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="{{(('tab-bar-item')+' '+(currentTab==item.id?'active':''))}}" data-current="{{index}}" data-event-opts="{{[['tap',[['swichNav',['$0'],[[['tabbar','',index,'id']]]]]]]}}" catchtap="__e"><text class="tab-bar-title">{{item.cate_name}}</text></view></block></scroll-view></view></view><view class="place"></view><view class="category"><block tt:for="{{categoryList}}" tt:for-item="category" tt:for-index="index" tt:key="index"><view hidden="{{!(category.id==showCategoryIndex)}}" class="banner"><image src="{{category.img}}"></image></view></block><view class="goodslist"><block tt:for="{{productList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="good-info"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','',index]]]]]]]}}" class="left-info" bindtap="__e"><image src="{{item.pro_sn}}" mode></image></view><view class="right-info"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','',index]]]]]]]}}" class="title title-two" bindtap="__e">{{item.pro_name}}</view><view class="price"><block tt:if="{{item.dis_price}}"><text data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','',index]]]]]]]}}" class="p" bindtap="__e">{{"¥"+item.dis_price+''}}<block tt:if="{{item.dis_price}}"><text class="dis_price" style="font-size:24rpx;text-decoration:line-through;">{{item.pro_price}}</text></block></text></block><block tt:else><text data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','',index]]]]]]]}}" class="p" bindtap="__e">{{"¥"+item.pro_price}}</text></block><view class="cartNum"><image class="cart" src="../../../static/img/tabBar/cart-on.png" mode data-event-opts="{{[['tap',[['joinCart',['$0'],[[['productList','',index]]]]]]]}}" bindtap="__e"></image><block tt:if="{{item.itemcount>0}}"><text class="num">{{item.itemcount}}</text></block></view></view></view></view></block></view></view><view class="loading-text">{{loadingText}}</view></scroll-view></view></view>