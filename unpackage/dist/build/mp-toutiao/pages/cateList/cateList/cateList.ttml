<view><view class="place"></view><scroll-view style="height:90%;" scroll-y="true" scroll-top="{{scrollTop}}" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]]]}}" bindscrolltolower="__e"><view class="goods-list"><view class="product-list"><block tt:for="{{goodsList}}" tt:for-item="goods" tt:for-index="index" tt:key="index"><view class="product"><image mode="widthFix" src="{{goods.pro_sn}}" data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','',index]]]]]]]}}" bindtap="__e"></image><view class="name">{{goods.pro_name}}</view><view class="info"><view class="price">{{goods.pro_price}}</view><image class="cart" src="../../../static/img/tabBar/cart-on.png" mode data-event-opts="{{[['tap',[['joinCart',['$0'],[[['goodsList','',index]]]]]]]}}" bindtap="__e"></image></view></view></block></view></view><view class="loading-text">{{loadingText}}</view></scroll-view></view>