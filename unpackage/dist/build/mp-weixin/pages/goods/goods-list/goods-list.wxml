<view><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><view class="input-box"><input placeholder="熙美诚品" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['blur',[['toSearch']]],['input',[['__set_model',['','key','$event',[]]]]]]}}" value="{{key}}" bindblur="__e" bindinput="__e"/><view class="icon search"></view></view></view><view class="place"></view><view class="goods-list"><view class="product-list"><block wx:for="{{goodsList}}" wx:for-item="goods" wx:for-index="index" wx:key="index"><view class="product"><image mode="widthFix" src="{{goods.pro_sn}}" data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','',index]]]]]]]}}" bindtap="__e"></image><view class="name">{{goods.pro_name}}</view><view class="info"><view class="price">{{goods.pro_price}}</view><image class="cart" src="../../../static/img/tabBar/cart-on.png" mode data-event-opts="{{[['tap',[['joinCart',['$0'],[[['goodsList','',index]]]]]]]}}" bindtap="__e"></image></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>